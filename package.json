{
  "name": "rabbitr",
  "version": "8.0.8",
  "description": "RabbitMQ made easy for nodejs",
  "main": "index",
  "scripts": {
    "typings": "typings install",
    "build": "tsc -p .",
    "test": "mocha -gc",
    "coverage": "istanbul cover _mocha --",
    "postcoverage": "if [ -f coverage/lcov.info ]; then node node_modules/.bin/map-coverage && mv coverage/lcov.info coverage/lcov-raw.info && mv coverage/lcov-mapped.info coverage/lcov.info; fi",
    "sendcoverage": "if [ -f coverage/lcov.info ]; then cat ./coverage/lcov.info | ./node_modules/codecov.io/bin/codecov.io.js; fi",
    "preversion": "npm run test -- --bail",
    "postversion": "git push --follow-tags",
    "declaration": "mkdir -p dist && tsc typings/main.d.ts index.ts --outDir dist/ --declaration --module commonjs --target es5 && mv dist/index.d.ts ./ && rm -rf dist/"
  },
  "author": "Giles Williams",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.4.1",
    "async": "^1.5.1",
    "bluebird": "^3.3.4",
    "chalk": "^1.1.1",
    "debug": "^2.2.0",
    "object-assign": "^4.0.1",
    "shortid": "^2.0.1"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "codecov.io": "^0.1.6",
    "istanbul": "^0.4.0",
    "map-coverage": "0.0.3",
    "mocha": "^2.3.4",
    "typescript": "^1.8.7",
    "typings": "^0.7.9",
    "uuid": "^2.0.1"
  },
  "typings": "index.d.ts",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "keywords": [
    "rabbitmq",
    "messages",
    "queues",
    "amqp",
    "event",
    "subscribe",
    "schedule",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/urbanmassage/node-rabbitr.git"
  },
  "bugs": {
    "url": "https://github.com/urbanmassage/node-rabbitr/issues"
  },
  "homepage": "https://github.com/urbanmassage/node-rabbitr"
}
