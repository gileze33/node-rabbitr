{
  "name": "rabbitr",
  "version": "10.2.1",
  "description": "RabbitMQ made easy for nodejs",
  "main": "index",
  "scripts": {
    "typings": "typings install && typings prune",
    "build": "npm run typings && tsc -p .",
    "test": "DEBUG=rabbit* mocha -gc",
    "coverage": "nyc --reporter=lcov --reporter=text mocha",
    "sendcoverage": "if [ -f coverage/lcov.info ]; then cat ./coverage/lcov.info | ./node_modules/codecov.io/bin/codecov.io.js; fi",
    "preversion": "npm run test -- --bail",
    "postversion": "git push --follow-tags"
  },
  "author": "Giles Williams",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.5.2",
    "bluebird": "^3.4.1",
    "chalk": "^1.1.3",
    "debug": "^2.6.8",
    "node-uuid": "^1.4.8",
    "object-assign": "^4.1.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codecov.io": "^0.1.6",
    "mocha": "^2.5.3",
    "nyc": "^7.1.0",
    "source-map-support": "^0.4.15",
    "typescript": "^1.8.0",
    "typings": "^2.0.0"
  },
  "typings": "index.d.ts",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "keywords": [
    "rabbitmq",
    "messages",
    "queues",
    "amqp",
    "event",
    "subscribe",
    "schedule",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/urbanmassage/node-rabbitr.git"
  },
  "bugs": {
    "url": "https://github.com/urbanmassage/node-rabbitr/issues"
  },
  "homepage": "https://github.com/urbanmassage/node-rabbitr"
}
