{
  "name": "rabbitr",
  "version": "8.3.0",
  "description": "RabbitMQ made easy for nodejs",
  "main": "index",
  "scripts": {
    "typings": "typings install && typings prune",
    "build": "tsc -p .",
    "test": "mocha -gc",
    "coverage": "istanbul cover _mocha --",
    "postcoverage": "if [ -f coverage/lcov.info ]; then node node_modules/.bin/map-coverage && mv coverage/lcov.info coverage/lcov-raw.info && mv coverage/lcov-mapped.info coverage/lcov.info; fi",
    "sendcoverage": "if [ -f coverage/lcov.info ]; then cat ./coverage/lcov.info | ./node_modules/codecov.io/bin/codecov.io.js; fi",
    "preversion": "npm run test -- --bail",
    "postversion": "git push --follow-tags"
  },
  "author": "Giles Williams",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.4.2",
    "bluebird": "^3.4.1",
    "chalk": "^1.1.3",
    "debug": "^2.2.0",
    "node-uuid": "^1.4.7",
    "object-assign": "^4.1.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codecov.io": "^0.1.6",
    "istanbul": "^0.4.4",
    "map-coverage": "0.0.3",
    "mocha": "^2.5.3",
    "source-map-support": "^0.4.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.1"
  },
  "typings": "index.d.ts",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "keywords": [
    "rabbitmq",
    "messages",
    "queues",
    "amqp",
    "event",
    "subscribe",
    "schedule",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/urbanmassage/node-rabbitr.git"
  },
  "bugs": {
    "url": "https://github.com/urbanmassage/node-rabbitr/issues"
  },
  "homepage": "https://github.com/urbanmassage/node-rabbitr"
}
